{
    "manifest_version": 3,
    "name": "UPSC Helper",
    "version": "1.0",
    "description": "Get a historical events with dates every time you open a new tab. Perfect for UPSC exam preparation.",
    "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
  }
  